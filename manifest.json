{
   "author": "小影",
   "background": {
      "scripts": [ "js/vendor/llqrcode.js", "js/vendor/qrcode.min.js", "background.js" ]
   },
   "default_locale": "en",
   "description": "__MSG_description__",
   "homepage_url": "http://path/to/homepage",
   "icons": {
      "128": "icon/128.png",
      "16": "icon/16.png",
      "32": "icon/32.png",
      "48": "icon/48.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "35",
   "name": "__MSG_name__",
   "offline_enabled": true,
   "options_page": "options.html",
   "options_ui": {
      "chrome_style": true,
      "page": "options.html"
   },
   "page_action": {
      "default_icon": {
         "19": "icon/19.png",
         "38": "icon/38.png"
      },
      "default_popup": "popup.html",
      "default_title": "__MSG_generate__"
   },
   "permissions": [ "activeTab", "contextMenus", "http://*/*", "https://*/*" ],
   "version": "1.1.5"
}
